    <owl:Class rdf:about="#Archaic_Lyric_Poetess">
        <rdfs:label xml:lang="en">archaic lyric poetess</rdfs:label>
        <skos:prefLabel xml:lang="en">archaic lyric poetess</skos:prefLabel>
        <skos:definition xml:lang="en">An Archaic Lyric poetess (woman) is a person who lived during 7th-early 5th century BCE and wrote Archaic Lyric poetry.</skos:definition>
