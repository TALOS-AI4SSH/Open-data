        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#in_ionic_or_in_doric_or_in_aeolic_dialect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

