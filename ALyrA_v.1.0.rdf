    <otv:Term rdf:about="#archaic_lyric_poem_en">
        <rdfs:label xml:lang="en">archaic lyric poem</rdfs:label>
        <otv:termName>archaic lyric poem</otv:termName>
        <otv:language>en</otv:language>
