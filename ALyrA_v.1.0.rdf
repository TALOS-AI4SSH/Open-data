    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_with_imitation_expressions">
        <rdfs:label xml:lang="en">hyporcheme</rdfs:label>
        <skos:prefLabel xml:lang="en">hyporcheme</skos:prefLabel>
        <skos:definition xml:lang="en">A lyric song accompanied by imitative movements. Bacchylides and Pindar are famous for composing hyporchemes.</skos:definition>
