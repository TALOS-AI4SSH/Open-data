                <owl:hasValue rdf:resource="#dedicated_to_a_dead_being"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect">
        <rdfs:label xml:lang="en">melos</rdfs:label>
        <skos:prefLabel xml:lang="en">melos</skos:prefLabel>
        <skos:definition xml:lang="en">Melos is the hypernym of the Archaic Lyric poems, which were written in ionic (except of the iamb and elegy), aeolic and doric dialect.</skos:definition>
        <rdfs:label xml:lang="el">μέλος</rdfs:label>
        <skos:prefLabel xml:lang="el">μέλος</skos:prefLabel>
        <skos:definition xml:lang="el">Το Μέλος είναι το υπερώνυμο των αρχαϊκών λυρικών ποιημάτων, τα οποία ήταν γραμμένα σε ιωνική (εκτός από τον ίαμβο και την ελεγεία), αιολική και δωρική διάλεκτο.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem"/>
