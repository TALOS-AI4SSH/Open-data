        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_single_performer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#performed_by_a_chorus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_on_the_way_to_an_altar">
        <rdfs:label xml:lang="en">prosodion</rdfs:label>
        <skos:prefLabel xml:lang="en">prosodion</skos:prefLabel>
        <skos:definition xml:lang="en">A lyric song performed on the way to an altar by a chorus. Alcman, Bacchylides and Pindar are famous prosodia composers.</skos:definition>
        <rdfs:label xml:lang="el">προσόδιο</rdfs:label>
        <skos:prefLabel xml:lang="el">προσόδιο</skos:prefLabel>
        <skos:definition xml:lang="el">Ένα λυρικό τραγούδι που ερμηνεύεται στο δρόμο προς το ιερό από μια χορωδία. Ο Αλκμάν, ο Βακχυλίδης και ο Πίνδαρος είναι διάσημοι συνθέτες προσοδίων.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_performed_by_a_non-specific_type_of_chorus"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_during_bride_procession"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#on_the_way_to_an_altar"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_single_performer">
        <rdfs:label xml:lang="en">monody</rdfs:label>
        <skos:prefLabel xml:lang="en">monody</skos:prefLabel>
        <skos:definition xml:lang="en">Monodies are characterized by solo performances, where the poet identifies with the singer and expresses personal thoughts and emotions, as they offer a direct and free expression of personality. For instance, in Sapphic odes, the poetess prioritizes her own voice over others.</skos:definition>
        <rdfs:label xml:lang="el">μονωδία</rdfs:label>
        <skos:prefLabel xml:lang="el">μονωδία</skos:prefLabel>
        <skos:definition xml:lang="el">Οι μονωδίες χαρακτηρίζονται από σόλο ερμηνείες, όπου ο ποιητής ταυτίζεται με τον τραγουδιστή και εκφράζει προσωπικές σκέψεις και συναισθήματα, καθώς προσφέρουν μια άμεση και ελεύθερη έκφραση της προσωπικότητας. Για παράδειγμα, στις σαπφικές ωδές, η ποιήτρια δίνει προτεραιότητα στη δική της φωνή έναντι των άλλων.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#performed_by_a_single_performer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_only_in_ionic_dialect">
        <rdfs:label xml:lang="en">archaic lyric poem only in ionic dialect</rdfs:label>
        <skos:prefLabel xml:lang="en">archaic lyric poem only in ionic dialect</skos:prefLabel>
        <skos:definition xml:lang="en">Archaic Lyric poem written only in ionic dialect. Ionic greek dialect, a subset of Eastern Greek, originated on the mainland and spread during the Dorian invasions. By the 5th century BC, it was primarily spoken in Ionia, including Asia Minor, Chios, and Samos. It also extended to Euboea and through Ionian colonization to the northern Aegean, Black Sea, and western Mediterranean.</skos:definition>
        <rdfs:label xml:lang="el">αρχαϊκό λυρικό ποίημα μόνο στην ιωνική διάλεκτο</rdfs:label>
        <skos:prefLabel xml:lang="el">αρχαϊκό λυρικό ποίημα μόνο στην ιωνική διάλεκτο</skos:prefLabel>
        <skos:definition xml:lang="el">Αρχαϊκό λυρικό ποίημα μόνο στην ιωνική διάλεκτο.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
