            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_dionysus">
        <rdfs:label xml:lang="en">dithyramb</rdfs:label>
        <skos:prefLabel xml:lang="en">dithyramb</skos:prefLabel>
        <skos:definition xml:lang="en">Devotional hymns of Dionysus. Bacchylides, Pindar and Simonides are known to have composed dithyrambs.</skos:definition>
        <rdfs:label xml:lang="el">διθύραμβος</rdfs:label>
        <skos:prefLabel xml:lang="el">διθύραμβος</skos:prefLabel>
        <skos:definition xml:lang="el">Λατρευτικοί ύμνοι του Διονύσου. Ο Βακχυλίδης, ο Πίνδαρος και ο Σιμωνίδης είναι γνωστό ότι συνέθεσαν διθύραμβους.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_a_god"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_apollo"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#dedicated_to_dionysus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_a_god">
        <rdfs:label xml:lang="en">hymn</rdfs:label>
        <skos:prefLabel xml:lang="en">hymn</skos:prefLabel>
        <skos:definition xml:lang="en">Lyric song dedicated to gods. Multiple poets has composed hymns, sych as Alcaeus, Terpander, Sappho, Alcman et. al.</skos:definition>
        <rdfs:label xml:lang="el">ύμνος</rdfs:label>
        <skos:prefLabel xml:lang="el">ύμνος</skos:prefLabel>
        <skos:definition xml:lang="el">Λυρικό τραγούδι αφιερωμένο στους θεούς. Πολλοί ποιητές έχουν συνθέσει ύμνους, όπως ο Αλκαίος, ο Τέρπανδρος, η Σαπφώ, ο Αλκμάνας κ.ά.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_living"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#dedicated_to_a_god"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_iambic_or_trochaic_metre">
        <rdfs:label xml:lang="en">iamb</rdfs:label>
        <skos:prefLabel xml:lang="en">iamb</skos:prefLabel>
        <skos:definition xml:lang="en">Initially associated with Dionysus, the iamb may have a pre-Hellenic origin as a poetic form. Typically composed in iambic trimeter or trochaic tetrameter, its Ionic version often adopts a first-person perspective, though its context can vary.</skos:definition>
        <rdfs:label xml:lang="el">ίαμβος</rdfs:label>
        <skos:prefLabel xml:lang="el">ίαμβος</skos:prefLabel>
        <skos:definition xml:lang="el">Αρχικά συνδεόμενος με τον Διόνυσο, ο ίαμβος μπορεί να έχει προελληνική προέλευση ως ποιητική μορφή. Συνήθως συντίθεται σε ιαμβικό τρίμετρο ή τροχαϊκό τετράμετρο, η ιωνική εκδοχή του συχνά υιοθετεί την πρωτοπρόσωπη σκοπιά, αν και τα συμφραζόμενα μπορεί να ποικίλλουν.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#in_iambic_or_trochaic_metre"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer">
        <rdfs:label xml:lang="en">archaic lyric poem in ionic or in doric or in aeolic dialect performed by a chorus or by a single performer</rdfs:label>
        <skos:prefLabel xml:lang="en">archaic lyric poem in ionic or in doric or in aeolic dialect performed by a chorus or by a single performer</skos:prefLabel>
        <skos:definition xml:lang="en">Melos which was performed either by one person or by a chorus.</skos:definition>
        <rdfs:label xml:lang="el">αρχαϊκό λυρικό ποίημα σε ιωνική ή δωρική ή αιολική διάλεκτο που εκτελείται από χορό ή από έναν μόνο ερμηνευτή</rdfs:label>
        <skos:prefLabel xml:lang="el">αρχαϊκό λυρικό ποίημα σε ιωνική ή δωρική ή αιολική διάλεκτο που εκτελείται από χορό ή από έναν μόνο ερμηνευτή</skos:prefLabel>
        <skos:definition xml:lang="el">Μέλος που εκτελείται από χορό ή από έναν μόνο ερμηνευτή.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_single_performer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#performed_by_a_chorus_or_by_a_single_performer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_dead">
        <rdfs:label xml:lang="en">threnos</rdfs:label>
        <skos:prefLabel xml:lang="en">threnos</skos:prefLabel>
        <skos:definition xml:lang="en">Lyric song dedicated to a dead human being. Anacreon, Pindar and Simonides are famous for their such compositions.</skos:definition>
        <rdfs:label xml:lang="el">θρήνος</rdfs:label>
        <skos:prefLabel xml:lang="el">θρήνος</skos:prefLabel>
        <skos:definition xml:lang="el">Λυρικό τραγούδι αφιερωμένο σε έναν νεκρό άνθρωπο. Ο Ανακρέων, ο Πίνδαρος και ο Σιμωνίδης είναι διάσημοι για τέτοιες συνθέσεις τους.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_living"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_wedding_couple"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_winners_in_athletic_events"/>
        <rdfs:subClassOf>
            <owl:Restriction>
