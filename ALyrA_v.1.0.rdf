    <owl:Class rdf:about="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet">
        <rdfs:label xml:lang="en">elegy</rdfs:label>
        <skos:prefLabel xml:lang="en">elegy</skos:prefLabel>
        <skos:definition xml:lang="en">Elegies are poems written in elegiac couplets, with the first line in hexameter and the second line in pentameter, initially used for lamentation (but later had a wide range of themes) and possibly originating from Eastern mourning practices. They can be as short as a single couplet or longer sequences as needed.</skos:definition>
        <rdfs:label xml:lang="el">ελεγεία</rdfs:label>
        <skos:prefLabel xml:lang="el">ελεγεία</skos:prefLabel>
        <skos:definition xml:lang="el">Οι ελεγείες είναι ποιήματα γραμμένα σε ελεγειακά δίστιχα, με τον πρώτο στίχο σε εξάμετρο και τον δεύτερο σε πεντάμετρο. Αρχικά, χρησιμοποιήθηκαν για θρήνους (αλλά αργότερα είχαν ένα ευρύ φάσμα θεμάτων) και πιθανόν προέρχονται από τις ανατολίτικες θρηνητικές πρακτικές. Μπορεί να είναι μικρές σε έκταση (όσο ένα απλό κουπλέ) ή μεγαλύτερες.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_iambic_or_trochaic_metre"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#in_elegiac_couplet"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem">
        <rdfs:label xml:lang="en">archaic lyric poem</rdfs:label>
        <skos:prefLabel xml:lang="en">archaic lyric poem</skos:prefLabel>
        <skos:definition xml:lang="en">"Lyric poetry began as a fixture of ancient Greece, classified against other categories of poetry at the time of classical antiquity: dramas (written in verse) and epic poems. The lyric was far shorter, distinguished also by its focus on the poet’s state of mind and personal themes rather than narrative arc" (Academy of American Poets). So, an Archaic Lyric Poem is such poem written from 7th-early 5th century BCE.</skos:definition>
        <rdfs:label xml:lang="el">αρχαϊκό λυρικό ποίημα</rdfs:label>
        <skos:prefLabel xml:lang="el">αρχαϊκό λυρικό ποίημα</skos:prefLabel>
        <skos:definition xml:lang="el">"Η Λυρική ποίηση ξεκίνησε ως θεσμός της αρχαίας Ελλάδας, ταξινομημένος σε σχέση με άλλες κατηγορίες ποίησης κατά την εποχή της κλασικής αρχαιότητας: τα δραματικά (γραμμένα σε στίχους) και τα επικά ποιήματα. Η λυρική ποίηση ήταν πολύ πιο σύντομη, ενώ διακρινόταν επίσης από την εστίασή της στην ψυχική κατάσταση του ποιητή και σε προσωπικά θέματα και όχι στο αφηγηματικό τόξο" (Ακαδημία Αμερικανών Ποιητών). Έτσι, ένα αρχαϊκό λυρικό ποίημα είναι ένα τέτοιο ποίημα που γράφτηκε από τον 7ο-αρχές του 5ου αιώνα π.Χ.</skos:definition>
        <owl:disjointWith rdf:resource="#Ancient_Location"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Composer"/>
        <owl:disjointWith rdf:resource="#BCE_century"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#archaic_lyric_poem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isWrittenBy"/>
                <owl:someValuesFrom rdf:resource="#Archaic_Lyric_Composer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_apollo">
        <rdfs:label xml:lang="en">paean</rdfs:label>
        <skos:prefLabel xml:lang="en">paean</skos:prefLabel>
        <skos:definition xml:lang="en">Lyric song dedicated to Ancient Greek god Apollo. Alcman, Pindar and Bacchylides are famous composers of paeans.</skos:definition>
        <rdfs:label xml:lang="el">παιάνας</rdfs:label>
        <skos:prefLabel xml:lang="el">παιάνας</skos:prefLabel>
        <skos:definition xml:lang="el">Λυρικό τραγούδι αφιερωμένο στον αρχαίο Έλληνα θεό Απόλλωνα. Ο Αλκμάνας, ο Πίνδαρος και ο Βακχυλίδης είναι διάσημοι συνθέτες παιάνων.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_a_god"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus_or_by_a_single_performer_dedicated_to_dionysus"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#dedicated_to_apollo"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet_with_gnomic_content">
        <rdfs:label xml:lang="en">gnomic elegy</rdfs:label>
        <skos:prefLabel xml:lang="en">gnomic elegy</skos:prefLabel>
        <skos:definition xml:lang="en">Elegy with gnomic, counseling, biosophical content. Known poets of gnomic elegies are Phocylides and Theognis.</skos:definition>
        <rdfs:label xml:lang="el">γνωμική ελεγεία</rdfs:label>
        <skos:prefLabel xml:lang="el">γνωμική ελεγεία</skos:prefLabel>
        <skos:definition xml:lang="el">Ελεγεία με γνωμικό, συμβουλευτικό, βιοσοφικό περιεχόμενο. Γνωστοί ποιητές γνωστών γνωμικών ελεγειών είναι ο Φωκυλίδης και ο Θεογένης.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet_with_erotic_content"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet_with_political_content"/>
        <owl:disjointWith rdf:resource="#Archaic_Lyric_Poem_only_in_ionic_dialect_in_elegiac_couplet_with_war_content"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#with_specific_content"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ownDifference"/>
                <owl:hasValue rdf:resource="#with_gnomic_content"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect_performed_by_a_chorus">
        <rdfs:label xml:lang="en">archaic lyric poem in ionic or in doric or in aeolic dialect performed by a chorus</rdfs:label>
        <skos:prefLabel xml:lang="en">archaic lyric poem in ionic or in doric or in aeolic dialect performed by a chorus</skos:prefLabel>
        <skos:definition xml:lang="en">Lyric song performed by a group (chorus) of people (women, men), which were dancing and singing.</skos:definition>
        <rdfs:label xml:lang="el">αρχαϊκό λυρικό ποίημα σε ιωνική ή δωρική ή αιολική διάλεκτο που εκτελείται από χορό</rdfs:label>
        <skos:prefLabel xml:lang="el">αρχαϊκό λυρικό ποίημα σε ιωνική ή δωρική ή αιολική διάλεκτο που εκτελείται από χορό</skos:prefLabel>
        <skos:definition xml:lang="el">Μέλος που εκτελείται από Χορό.</skos:definition>
        <rdfs:subClassOf rdf:resource="#Archaic_Lyric_Poem_in_ionic_or_in_doric_or_in_aeolic_dialect"/>
