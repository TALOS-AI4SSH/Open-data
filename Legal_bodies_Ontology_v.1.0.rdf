        <rdfs:label xml:lang="en">Eponymous Archon</rdfs:label>
        <skos:prefLabel xml:lang="en">Eponymous Archon</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, serves a one year term, supervises family lawsuits for citizens, held by one person.</skos:definition>
        <rdfs:label xml:lang="grc">ἐπώνυμος ἄρχων</rdfs:label>
        <skos:prefLabel xml:lang="grc">ἐπώνυμος ἄρχων</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων chosen by lot, serves a one year term, supervises family lawsuits for citizens, held by one person.</skos:definition>
        <skos:example xml:lang="grc">Dem. 21.9: "ἐν δὲ ταύτῃ ἐπειδὰν χρηματίσωσιν οἱ πρόεδροι περὶ ὧν διῴκηκεν ὁ ἄρχων, χρηματίζειν καὶ περὶ ὧν ἄν τις ἠδικηκὼς ᾖ περὶ τὴν ἑορτὴν ἢ παρανενομηκώς"</skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_chosen_by_lot_serves_a_one_year_term_supervises_private_actions_and_family_law_for_metics">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Polemarch</rdfs:label>
        <skos:prefLabel xml:lang="en">Polemarch</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, serves a one year term, supervises private actions and family law for metics, held by one person.</skos:definition>
        <rdfs:label xml:lang="grc">πολέμαρχος</rdfs:label>
        <skos:prefLabel xml:lang="grc">πολέμαρχος</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων chosen by lot, serves a one year term, supervises private actions and family law for metics, held by one person.</skos:definition>
        <skos:example xml:lang="grc">Dem. 59.40: "ἀφαιρουμένου δὲ τοῦ Στεφάνου κατὰ τὸν νόμον εἰς ἐλευθερίαν, κατηγγύησεν αὐτὴν πρὸς τῷ πολεμάρχῳ."</skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group_chosen_by_lot_has_51_members_serves_a_one_year_term_judges_unintentional_and_justifiable_homicide_cases">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Ephetai</rdfs:label>
        <skos:prefLabel xml:lang="en">Ephetai</skos:prefLabel>
        <skos:definition xml:lang="en">Council chosen by lot, has 51 members, serves a one year term, judges unintentional and justifiable homicide cases.</skos:definition>
        <rdfs:label xml:lang="grc">ἐφέται</rdfs:label>
        <skos:prefLabel xml:lang="grc">ἐφέται</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ chosen by lot, has 51 members, serves a one year term, judges unintentional and justifiable homicide cases.</skos:definition>
        <skos:example xml:lang="grc">Dem. 23.37: "ἐὰν δέ τις τὸν ἀνδροφόνον κτείνῃ ἢ αἴτιος ᾖ φόνου, ἀπεχόμενον ἀγορᾶς ἐφορίας καὶ ἄθλων καὶ ἱερῶν Ἀμφικτυονικῶν, ὥσπερ τὸν Ἀθηναῖον κτείναντα, ἐν τοῖς αὐτοῖς ἐνέχεσθαι, διαγιγνώσκειν δὲ τοὺς ἐφέτας."</skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

