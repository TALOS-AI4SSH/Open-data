	<!--  SKOS Concept Scheme -->

    <skos:ConceptScheme rdf:about="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0">
        <dc:title>Athenian-Legal-Bodies</dc:title>
        <dc:description>This is version 1.0 of a dataset created in the context  of a PhD thesis in "Semantic Annotation and Attic Oratory" that defines the primary legal bodies in Classical Athenian courts c. 419 BC to 323 BC. A legal body is defined as "an individual or group who either proceeds over or judges a legal proceeding". This work draws upon the corpus of Attic oratory and the Pseudo-Aristotelian Athenian constitution, as well as The Law of Ancient Athens by David D. Phillips.
This dataset is authored by Rachel Milio (TALOS Ph.D. candidate, Department of Philology, University of Crete), under the supervision of Dr. Maria Papadopoulou (Department of Philology, University of Crete) and Prof. Christophe Roche (TALOS ERA Chair Professor, University of Crete).
Acknowledgement: We gratefully acknowledge the generous funding of the European Commission under the ERA Chair Project TALOS AI for SSH (Grant agreement ID: 101087269).
Contact: Rachel.Milio@outlook.com
</dc:description>
        <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-8-6</dc:issued>
        <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-8-28</dc:modified>
        <dc:creator>Rachel Milio</dc:creator>
        <dc:publisher>University of Crete</dc:publisher>
    </skos:ConceptScheme>

	<!--  SKOS Concepts -->

    <skos:Concept rdf:about="#Group_chosen_by_lot_has_a_size_that_varies_based_on_case_serves_a_one_year_term_judge_the_majority_of_legal_proceedings">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Law courts</rdfs:label>
        <skos:prefLabel xml:lang="en">Law courts</skos:prefLabel>
        <skos:definition xml:lang="en">Council chosen by lot, has a size that varies based on case, serves a one year term, judge the majority of legal proceedings.</skos:definition>
        <rdfs:label xml:lang="grc">δικαστήριον</rdfs:label>
        <skos:prefLabel xml:lang="grc">δικαστήριον</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ chosen by lot, has a size that varies based on case, serves a one year term, judge the majority of legal proceedings.</skos:definition>
        <skos:example xml:lang="grc">Dem. 21.221: "αὐτίκα δὴ μάλα, ἐπειδὰν ἀναστῇ τὸ δικαστήριον, εἷς ἕκαστος ὑμῶν"</skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group_has_50_members_serves_for_one_tenth_of_a_year_made_up_of_council_members_in_an_order_determined_by_lot_convenes_the_Council_and_Assembly">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Prytaneis</rdfs:label>
        <skos:prefLabel xml:lang="en">Prytaneis</skos:prefLabel>
        <skos:definition xml:lang="en">Council has 50 members, serves for one tenth of a year, made up of council members in an order determined by lot, convenes the Council and Assembly.</skos:definition>
        <rdfs:label xml:lang="en">Presidents</rdfs:label>
        <skos:altLabel xml:lang="en">Presidents</skos:altLabel>
        <skos:definition xml:lang="en">Council has 50 members, serves for one tenth of a year, made up of council members in an order determined by lot, convenes the Council and Assembly.</skos:definition>
        <rdfs:label xml:lang="grc">πρυτάνεις</rdfs:label>
        <skos:prefLabel xml:lang="grc">πρυτάνεις</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ has 50 members, serves for one tenth of a year, made up of council members in an order determined by lot, convenes the Council and Assembly.</skos:definition>
        <skos:example xml:lang="grc">Aeschin. 2.60: " προγράψαι τοὺς πρυτάνεις ἐκκλησίας δύο κατὰ τὸν νόμον, ἐν δὲ ταύταις βουλεύσασθαι περὶ τῆς εἰρήνης Ἀθηναίους"</skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <skos:topConceptOf rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Council</rdfs:label>
        <skos:prefLabel xml:lang="en">Council</skos:prefLabel>
        <skos:definition xml:lang="en">A group who judges trials.</skos:definition>
        <rdfs:label xml:lang="grc">Βουλὴ</rdfs:label>
        <skos:prefLabel xml:lang="grc">Βουλὴ</skos:prefLabel>
        <skos:definition xml:lang="grc">A group which judges legal proceedings.</skos:definition>
        <skos:example xml:lang="grc">Dem. 22.5: " νόμος ἐστί, φησίν, ἐὰν ἀξίως ἡ βουλὴ δοκῇ βουλεῦσαι δωρειᾶς, διδόναι τὸν δῆμον τὴν δωρειὰν αὐτῇ."</skos:example>
        <skos:narrower rdf:resource="#Group_chosen_by_lot_has_51_members_serves_a_one_year_term_judges_unintentional_and_justifiable_homicide_cases"/>
        <skos:narrower rdf:resource="#Group_becomes_a_member_after_archonship_serves_for_the_remainder_of_life_judges_suits_for_intentional_homicide_intentional_wounding_and_destruction_of_sacred_olive_trees_made_up_of_all_former_archons"/>
        <skos:narrower rdf:resource="#Group_deliberates_over_political_issues_made_up_of_all_adult_male_citizens_no_formal_selection_no_term_length"/>
        <skos:narrower rdf:resource="#Group_chosen_by_lot_has_500_members_serves_a_one_year_term_deliberates_over_agenda_for_Assembly_and_examines_appointed_magistrates"/>
        <skos:narrower rdf:resource="#Group_has_50_members_serves_for_one_tenth_of_a_year_made_up_of_council_members_in_an_order_determined_by_lot_convenes_the_Council_and_Assembly"/>
        <skos:narrower rdf:resource="#Group_chosen_by_lot_has_a_size_that_varies_based_on_case_serves_a_one_year_term_judge_the_majority_of_legal_proceedings"/>
        <skos:narrower rdf:resource="#Group_chosen_by_lot_has_40_members_serves_a_one_year_term_judges_private_lawsuits_over_up_to_10_drachmas"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_chosen_by_lot_held_by_6_people_serves_a_one_year_term_supervises_lawsuits_against_other_officials_such_as_impeachments_indictments_for_illegal_proposals_and_scrutinies">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Law-Givers</rdfs:label>
        <skos:altLabel xml:lang="en">Law-Givers</skos:altLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, held by 6 people, serves a one year term, supervises lawsuits against other officials such as impeachments indictments for illegal proposals and scrutinies.</skos:definition>
        <rdfs:label xml:lang="en">Thesmothetai</rdfs:label>
        <skos:prefLabel xml:lang="en">Thesmothetai</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, held by 6 people, serves a one year term, supervises lawsuits against other officials such as impeachments indictments for illegal proposals and scrutinies.</skos:definition>
        <rdfs:label xml:lang="grc">θεσμοθέται</rdfs:label>
        <skos:prefLabel xml:lang="grc">θεσμοθέται</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων chosen by lot, held by 6 people, serves a one year term, supervises lawsuits against other officials such as impeachments, indictments for illegal proposals, and scrutinies.</skos:definition>
        <skos:example xml:lang="grc">Isoc. 20.2: " ἔπειτα τῶν μὲν ἄλλων ἐγκλημάτων αὐτῷ τῷ παθόντι μόνον ὁ δράσας ὑπόδικός ἐστιν: περὶ δὲ τῆς ὕβρεως, ὡς κοινοῦ τοῦ πράγματος ὄντος, ἔξεστι τῷ βουλομένῳ τῶν πολιτῶν γραψαμένῳ πρὸς τοὺς θεσμοθέτας εἰσελθεῖν εἰς ὑμᾶς."</skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_held_by_11_people_chosen_by_lot_serves_a_one_year_term_supervises_arrests_imprisonments_and_executions">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Eleven</rdfs:label>
        <skos:prefLabel xml:lang="en">Eleven</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate held by 11 people, chosen by lot, serves a one year term, carries out arrests imprisonments and executions.</skos:definition>
        <rdfs:label xml:lang="grc">οἱ ἕνδεκα‎</rdfs:label>
        <skos:prefLabel xml:lang="grc">οἱ ἕνδεκα‎</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων held by 11 people, chosen by lot, serves a one year term, supervises arrests imprisonments and executions.</skos:definition>
        <skos:example xml:lang="grc">Isaeus 4.28: " οὗτος γάρ, ὅτ᾽ ἐπεδήμει ἐνθάδε, πρῶτον μὲν εἰς τὸ δεσμωτήριον ὡς κλέπτης ὢν ἐπ᾽ αὐτοφώρῳ ἀπήχθη, τότε δὲ ἀφεθεὶς μεθ᾽ ἑτέρων τινῶν ὑπὸ τῶν ἕνδεκα"</skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group_chosen_by_lot_has_500_members_serves_a_one_year_term_deliberates_over_agenda_for_Assembly_and_examines_appointed_magistrates">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Council of 500</rdfs:label>
        <skos:prefLabel xml:lang="en">Council of 500</skos:prefLabel>
        <skos:definition xml:lang="en">Council chosen by lot, has 500 members, serves a one year term, deliberates over agenda for Assembly and examines appointed magistrates.</skos:definition>
        <rdfs:label xml:lang="grc">βουλὴ οἱ πεντακόσιοι</rdfs:label>
        <skos:prefLabel xml:lang="grc">βουλὴ οἱ πεντακόσιοι</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ chosen by lot, has 500 members, serves a one year term, deliberates over agenda for Assembly and examines appointed magistrates.</skos:definition>
        <skos:example xml:lang="grc">Aeschin. 3.2: "ἐβουλόμην μὲν οὖν, ὦ ἄνδρες Ἀθηναῖοι, καὶ τὴν βουλὴν τοὺς πεντακοσίους καὶ τὰς ἐκκλησίας ὑπὸ τῶν ἐφεστηκότων ὀρθῶς διοικεῖσθαι"</skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_chosen_by_lot_serves_a_one_year_term_supervises_religious_suits_such_as_impiety_indictments_and_homicide_suits">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Basileus</rdfs:label>
        <skos:prefLabel xml:lang="en">Basileus</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, serves a one year term, supervises religious suits such as impiety indictments and homicide suits, held by one person.</skos:definition>
        <rdfs:label xml:lang="en">King</rdfs:label>
        <skos:altLabel xml:lang="en">King</skos:altLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, serves a one year term, supervises religious suits such as impiety indictments and homicide suits, held by one person.</skos:definition>
        <rdfs:label xml:lang="grc">βασιλεὺς</rdfs:label>
        <skos:prefLabel xml:lang="grc">βασιλεὺς</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων chosen by lot, serves a one year term, supervises religious suits such as impiety indictments and homicide suits, held by one person.</skos:definition>
        <skos:example xml:lang="grc">Lys. 6.4: " ἐὰν νυνὶ Ἀνδοκίδης ἀθῷος ἀπαλλαγῇ δι᾽ ὑμᾶς ἐκ τοῦδε τοῦ ἀγῶνος καὶ ἔλθῃ κληρωσόμενος τῶν ἐννέα ἀρχόντων καὶ λάχῃ βασιλεύς, ἄλλο τι ἢ ὑπὲρ ὑμῶν καὶ θυσίας θύσει καὶ εὐχὰς εὔξεται κατὰ τὰ πάτρια"</skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <skos:topConceptOf rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Magistrate</rdfs:label>
        <skos:prefLabel xml:lang="en">Magistrate</skos:prefLabel>
        <rdfs:label xml:lang="grc">ἄρχων</rdfs:label>
        <skos:prefLabel xml:lang="grc">ἄρχων</skos:prefLabel>
        <skos:definition xml:lang="grc">An official who supervises legal proceedings.</skos:definition>
        <skos:narrower rdf:resource="#Official_chosen_by_lot_serves_a_one_year_term_supervises_private_actions_and_family_law_for_metics"/>
        <skos:narrower rdf:resource="#Official_chosen_by_lot_serves_a_one_year_term_supervises_family_lawsuits_for_citizens"/>
        <skos:narrower rdf:resource="#Official_chosen_by_lot_serves_a_one_year_term_supervises_religious_suits_such_as_impiety_indictments_and_homicide_suits"/>
        <skos:narrower rdf:resource="#Official_chosen_by_lot_held_by_6_people_serves_a_one_year_term_supervises_lawsuits_against_other_officials_such_as_impeachments_indictments_for_illegal_proposals_and_scrutinies"/>
        <skos:narrower rdf:resource="#Official_held_by_11_people_chosen_by_lot_serves_a_one_year_term_supervises_arrests_imprisonments_and_executions"/>
        <skos:narrower rdf:resource="#Official_chosen_by_lot_held_by_10_people_serves_a_one_year_term"/>
        <skos:narrower rdf:resource="#Official_elected_from_each_tribe_held_by_10_people_serves_a_one_year_term_supervises_military_related_legal_proceedings"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group_chosen_by_lot_has_40_members_serves_a_one_year_term_judges_private_lawsuits_over_up_to_10_drachmas">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Forty</rdfs:label>
        <skos:prefLabel xml:lang="en">Forty</skos:prefLabel>
        <skos:definition xml:lang="en">Council chosen by lot, has 40 members, serves a one year term, judges private lawsuits over up to 10 drachmas.</skos:definition>
        <rdfs:label xml:lang="grc">τετταράκοντα</rdfs:label>
        <skos:prefLabel xml:lang="grc">τετταράκοντα</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ chosen by lot, has 40 members, serves a one year term, judges private lawsuits over up to 10 drachmas.</skos:definition>
        <skos:example xml:lang="grc">Isoc. 15.237: "ἐν γὰρ ταῖς σανίσι ταῖς ὑπὸ τῶν ἀρχόντων ἐκτιθεμέναις ἀναγκαῖόν ἐστιν, ἐν μὲν ταῖς ὑπὸ τῶν θεσμοθετῶν ἀμφοτέρους ἐνεῖναι, τούς τε τὴν πόλιν ἀδικοῦντας καὶ τοὺς συκοφαντοῦντας, ἐν δὲ ταῖς τῶν ἕνδεκα τούς τε κακουργοῦντας καὶ τοὺς τούτοις ἐφεστῶτας, ἐν δὲ ταῖς τῶν τετταράκοντα τούς τ᾽ ἐν τοῖς ἰδίοις πράγμασιν ἀδικοῦντας καὶ τοὺς μὴ δικαίως ἐγκαλοῦντας" </skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group_deliberates_over_political_issues_made_up_of_all_adult_male_citizens_no_formal_selection_no_term_length">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Assembly</rdfs:label>
        <skos:prefLabel xml:lang="en">Assembly</skos:prefLabel>
        <skos:definition xml:lang="en">Council deliberates over political issues, made up of all adult male citizens, no formal selection, no term length.</skos:definition>
        <rdfs:label xml:lang="grc">ἐκκλησία</rdfs:label>
        <skos:prefLabel xml:lang="grc">ἐκκλησία</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ deliberates over political issues, made up of all adult male citizens, no formal selection, no term length.</skos:definition>
        <skos:example xml:lang="grc">Dem. 25.9:  καὶ τὰ μὲν ἄλλ᾽ ἐάσω: ἀλλ᾽ εἰς τὰς ἐκκλησίας ἀναβαίνουσιν, ἐν αἷς ὑμεῖς γνώμης ἀπόδειξιν, οὐ πονηρίας τοῖς λέγουσι προτίθετε, τόλμαν καὶ κραυγὴν καὶ ψευδεῖς αἰτίας καὶ συκοφαντίαν καὶ ἀναισχυντίαν καὶ πάντα τὰ τοιαῦτα συνεσκευασμένοι, ὧν οὐκ ἂν εὕροι τις ἐναντιώτερα τῷ βουλεύεσθαι, νομίζω δ᾽ οὐδ᾽ ἂν αἰσχίω."</skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Group_becomes_a_member_after_archonship_serves_for_the_remainder_of_life_judges_suits_for_intentional_homicide_intentional_wounding_and_destruction_of_sacred_olive_trees_made_up_of_all_former_archons">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Council of the Areopagus</rdfs:label>
        <skos:prefLabel xml:lang="en">Council of the Areopagus</skos:prefLabel>
        <skos:definition xml:lang="en">Council becomes a member after archonship, serves for the remainder of life, judges suits for intentional homicide intentional wounding and destruction of sacred olive trees, made up of all former archons.</skos:definition>
        <rdfs:label xml:lang="grc">βουλὴ ἐξ Ἀρείου πάγου</rdfs:label>
        <skos:prefLabel xml:lang="grc">βουλὴ ἐξ Ἀρείου πάγου</skos:prefLabel>
        <skos:definition xml:lang="grc">Βουλὴ becomes a member after archonship, serves for the remainder of life, judges suits for intentional homicide intentional wounding and destruction of sacred olive trees, made up of all former archons.</skos:definition>
        <skos:example xml:lang="grc">Dem. 18.133: "καὶ εἰ μὴ ἡ βουλὴ ἡ ἐξ Ἀρείου πάγου τὸ πρᾶγμ᾽ αἰσθομένη καὶ τὴν ὑμετέραν ἄγνοιαν ἐν οὐ δέοντι συμβεβηκυῖαν ἰδοῦσα ἐπεζήτησε τὸν ἄνθρωπον καὶ συλλαβοῦσ᾽ ἐπανήγαγεν ὡς ὑμᾶς, ἐξήρπαστ᾽ ἂν ὁ τοιοῦτος καὶ τὸ δίκην δοῦναι διαδὺς ἐξεπέπεμπτ᾽ ἂν ὑπὸ τοῦ σεμνολόγου τουτουί:." </skos:example>
        <skos:broader rdf:resource="#Group"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_elected_from_each_tribe_held_by_10_people_serves_a_one_year_term_supervises_military_related_legal_proceedings">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Generals</rdfs:label>
        <skos:prefLabel xml:lang="en">Generals</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate elected from each tribe, held by 10 people, serves a one year term, supervises military related legal proceedings.</skos:definition>
        <rdfs:label xml:lang="grc">στρατηγοί</rdfs:label>
        <skos:prefLabel xml:lang="grc">στρατηγοί</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων elected from each tribe, held by 10 people, serves a one year term, supervises military related legal proceedings.</skos:definition>
        <skos:example xml:lang="grc">Dem. 4.25: " τὰς προφάσεις ἀφελεῖν καὶ τοῦ στρατηγοῦ καὶ τῶν στρατιωτῶν, μισθὸν πορίσαντας καὶ στρατιώτας οἰκείους ὥσπερ ἐπόπτας τῶν στρατηγουμένων παρακαταστήσαντας." </skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_chosen_by_lot_held_by_10_people_serves_a_one_year_term">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/Legal_proceedings_Ontology_v.1.0"/>
        <rdfs:label xml:lang="en">Archons</rdfs:label>
        <skos:prefLabel xml:lang="en">Archons</skos:prefLabel>
        <skos:definition xml:lang="en">Magistrate chosen by lot, held by 10 people, serves a one year term, hold preliminary hearings and assign cases.</skos:definition>
        <rdfs:label xml:lang="grc">ἄρχοντες</rdfs:label>
        <skos:prefLabel xml:lang="grc">ἄρχοντες</skos:prefLabel>
        <skos:definition xml:lang="grc">ἄρχων chosen by lot, held by 10 people, serves a one year term, hold preliminary hearings and assign cases.</skos:definition>
        <skos:example xml:lang="grc">Dem. 23.28: "εἰσφέρειν δ᾽ ἐς τοὺς ἄρχοντας, ὧν ἕκαστοι δικασταί εἰσι, τῷ βουλομένῳ. τὴν δ᾽ ἡλιαίαν διαγιγνώσκειν."</skos:example>
        <skos:broader rdf:resource="#Official"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Official_chosen_by_lot_serves_a_one_year_term_supervises_family_lawsuits_for_citizens">
